generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model User {
    id    String  @id @default(auto()) @map("_id") @db.ObjectId
    email String  @unique
    name  String?
}
